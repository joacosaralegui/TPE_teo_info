    "def desvio(imagen, media_imagen):\n",
    "    \"\"\"\n",
    "    desvio = sqrt(<x2> - <x>2)\n",
    "    \"\"\"\n",
    "    media_cuadrada_imagen = media(imagen,True)\n",
    "    varianza = media_cuadrada_imagen - (media_imagen*media_imagen)\n",
    "    desvio = math.sqrt(varianza)\n",
    "    return desvio"
   ]
